{
    "name":             "Gmail labels fixed position",
    "version":          "1.4",
    "manifest_version": 2,
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "description":      "Transform Gmail labels into a fixed bar as if they were glued on.",
    "content_scripts": [ {
        "matches":      ["*://*.mail.google.com/*"],
        "css":          ["default.css"],
        "js":           ["default.js"]
    } ]
}